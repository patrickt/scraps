[user]
	name = Patrick Thomson
[core]
	trustctime = false
	excludesfile = /Users/patrickthomson/.gitignore
	ignorecase = true
[gc]
	auto = 0
[diff] 
	renames = copies
	mnemonicprefix = true
[alias]
	st = status
	co = checkout
	br = branch
	fixup = commit -a --amend -C HEAD
[color]
  ui = true
  push
[grep]
	lineNumber = true
[alias]
	st = status
	co = checkout
	br = branch
	reup = rebase @{u}
[rerere]
	enabled = 1
[branch]
	autosetupmerge = true
	autosetuprebase = always
[advice]
	statusHints = false
[interactive]
	singlekey = true
